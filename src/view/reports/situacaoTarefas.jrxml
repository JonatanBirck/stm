	<parameter name="dataFinal" class="java.sql.Date"/>
	<queryString>
		<![CDATA[select TO_CHAR(t.date_create, 'DD/MM/YYYY') as datac, t.name as nome, u.name as cadastro, us.name as responsavel
from tasks t
join users u on (t.ref_user_created = u.id)
join users us on (t.ref_user_responsibility = us.id)
where t.state = $P{estado} and t.date_create BETWEEN  $P{dataInicio} AND $P{dataFinal} 
order by t.date_create ASC]]>
	</queryString>
